<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/editview_border_focus"
        android:state_pressed="true"
        android:state_focused="true"></item>
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
            <solid android:color="@color/themeColor2" />
            <corners android:radius="@dimen/editview_radius" />
            <stroke android:width="@dimen/editview_width"
                android:color="@color/themeColor" />
        </shape>
    </item>
</selector>
